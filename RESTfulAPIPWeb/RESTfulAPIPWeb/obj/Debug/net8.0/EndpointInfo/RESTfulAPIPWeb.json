  "components": {
    "schemas": {
      "AccessTokenResponse": {
        "required": [
          "accessToken",
          "expiresIn",
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "tokenType": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "accessToken": {
            "type": "string",
            "nullable": true
          },
          "expiresIn": {
            "type": "integer",
            "format": "int64"
          },
          "refreshToken": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Categoria": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nome": {
            "type": "string",
            "nullable": true
          },
          "ordem": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "urlImagem": {
            "type": "string",
            "nullable": true
          },
          "imagem": {
            "type": "string",
            "format": "byte",
            "nullable": true
          },
          "imageFile": {
            "type": "string",
            "format": "binary",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Encomenda": {
        "required": [
          "cidade",
          "codigoPostal",
          "localidade1",
          "pais",
          "rua"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "type": "string",
            "format": "date-time"
          },
          "estado": {
            "$ref": "#/components/schemas/EstadoEncomenda"
          },
          "pagamentoEfetuado": {
            "type": "boolean"
          },
          "pagamentoConfirmadoInternamente": {
            "type": "boolean"
          },
          "rua": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "localidade1": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "codigoPostal": {
            "minLength": 1,
            "pattern": "^\\d{4}-\\d{3}$",
            "type": "string"
          },
          "cidade": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "pais": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "produtosEncomendados": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EncomendaItem"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EncomendaItem": {
        "required": [
          "preco",
          "quantidade"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "quantidade": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "encomendaId": {
            "type": "integer",
            "format": "int32"
          },
          "produto": {
            "$ref": "#/components/schemas/Produto"
          },
          "nomeProduto": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "preco": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "detalheProduto": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "modoEntrega": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "detalheModoDisponibilizacao": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "nomeCategoria": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EstadoEncomenda": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "Favorito": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "clienteId": {
            "type": "string",
            "nullable": true
          },
          "produtoId": {
            "type": "integer",
            "format": "int32"
          }
        },
